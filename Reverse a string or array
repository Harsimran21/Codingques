class Solution {
public:
    void reverseString(vector<char>& arr) {
     char temp;
        int start=0;
        int end= arr.size() - 1;
        
        while(start<end){
                temp = arr[start];
                arr[start]= arr[end];
                arr[end]=temp;
                 start++;
                  end--;
        }
    }
};
